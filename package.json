{
  "scripts": {
    "build": "npm run --workspace 'ember-infinity' build",
    "lint": "npm run --workspace 'ember-infinity' --workspace 'test-app' lint",
    "lint:fix": "npm run --workspace 'ember-infinity' --workspace 'test-app' lint:fix",
    "prepare": "npm run build",
    "test:test-app": "npm run --workspace 'test-app' test:ember",
    "test:test-app:ember-try": "npm run --workspace 'test-app' test:ember-try",
    "start": "npm run --workspace 'test-app' start"
  },
  "private": true,
  "workspaces": [
    "ember-infinity",
    "test-app"
  ]
}
